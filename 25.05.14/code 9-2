q1 = {'김', '박', '최', '송', '이'} #1분기 실적 우수자
q2 = {'김', '박', '최', '홍', '강'} #2분기 실적 우수자

print('1분기 우수\t', q1)
print('2분기 우수\t', q2)
print('1분기만 우수\t', q1 - q2) #1분기는 우수이지만 2분기는 우수가 아닌 사원  
print('2분기만 우수\t', q2 - q1)
print('1분기나 2분기 중 한 번만 우수\t', q1 ^ q2) #^는 두 집합의 합집합에서 교집합을 뺀 것과 결과가 같다.
print('1분기 또는 2분기 우수\t', q1 | q2) #|는 합집합

bonus_person = q1 & q2
print('1분기와 2분기 모두 우수\t', bonus_person)

person = input('\n두 분기 중 최고 실적 사원은?')
bonus_person.add(person)

print('\n상반기 보너스 대상자\t', bonus_person)
